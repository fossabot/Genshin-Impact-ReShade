echo ========================================================================================= & echo.

echo 1/2 - Checking administrative permissions...
net session >nul 2>&1
if "%ERRORLEVEL%"=="0" (
    echo [✓] No problems found. & echo.
) else (
    echo [x] This file needs to be executed with administrative privileges.
    goto done
)

set /p "response=» Are you sure? [Yes/no]: "
if /i "%response%"=="yes" (
    echo You chose "Yes". Deleting WebView2 cache in progress...
) else (
    echo You chose "No" or provided an invalid response. Paused.
    goto done
)
echo.




echo 2/2 - Deleting WebView2 cache...
set "WebViewCache=%~4\EBWebView"
echo [i] Path: %WebViewCache%
if exist "%WebViewCache%" (
    rd /s /q "%WebViewCache%"
    echo [✓] Success.
) else (
    echo [x] Not found.
)
echo.




echo [i] Done! Operation has been completed.
goto done

:done
    echo.
    set /p null=
    exit
